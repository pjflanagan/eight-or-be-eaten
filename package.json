{
  "name": "octopus",
  "scripts": {
    "dev-webpack": "npx webpack --watch",
    "dev-scss": "npx sass --watch ./styles/index.scss ./dist/style.css",
    "dev": "npm run dev-webpack & npm run dev-scss",
    "build-webpack": "npx webpack",
    "build-scss": "npx sass ./styles/index.scss ./dist/style.css",
    "build": "npm run build-webpack && npm run build-scss",
    "open": "open ./index.html"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "phaser": "^3.80.1",
    "phaser-matter-collision-plugin": "^1.0.0"
  }
}
